{
    "version": 2,
    "builds": [{
        "src": "index.js",
        "use": "@now/node"
    }],
    "routes": [{
        "src": "/.*",
        "dest": "/main.js"
    }],
    "env": {
        "HASH": "@hash",
        "API_KEY": "@api_key",
        "TIMESTAMP": "@timestamp",
        "PORT": "@port"
    }
}